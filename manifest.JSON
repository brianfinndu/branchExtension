{
  "manifest_version": 3,
  "name": "Branch",
  "description": "Base Level Extension",
  "version": "1.0",
  "permissions": [
    "tabs",
    "storage",
    "webNavigation",
    "scripting",
    "contextMenus",
    "identity",
    "activeTab"
  ],
  "oauth2": {
    "client_id": "132228782829-c6vmuho5mq2kdculo63apih6vbumpev2.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/drive.file"]
  },
  "action": {
    "default_popup": "branch.html",
    "default_icon": "images/branchLogo.png"
  },
  "commands": {
    "toggle-tree-overlay": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S"
      },
      "description": "Toggle the tree overlay"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["treeRender.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "key": "",
  "icons": {
    "16": "images/branchLogo.png"
  },

  "background": {
    "service_worker": "background.js",
    "type": "module"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start",
      "type": "module"
    }
  ]
}
