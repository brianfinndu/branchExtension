{
  "manifest_version": 3,
  "name": "Branch Extension",
  "version": "1.0",
  "description": "Save and sync URLs using Google Drive",
  "permissions": [
    "identity",
    "identity.email",
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://www.googleapis.com/*"
  ],
  "oauth2": {
    "client_id": "532808127148-i1vm5tplqemsog8guuf0fkmgfdjck3co.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive",
      "https://www.googleapis.com/auth/drive.file",
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/drive.appdata"
    ]
  },
  "background": {
    "service_worker": "googleDrive.js"
  },
  "action": {
    "default_popup": "branch.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["saveLink.js"]
    }
  ]
}


